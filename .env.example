# 🔑 Steam API Key
STEAM_API_KEY= # Your Steam Web API key

# 🌐 Steam OAuth URLs
STEAM_REALM= # Your public URL or IP (e.g. http://yourdomain.com)
STEAM_RETURN_URL= # OAuth return endpoint (e.g. http://yourdomain.com/auth/steam/return)

# 🤖 Discord Bot Credentials
DISCORD_TOKEN= # Your Discord bot token
CLIENT_ID= # Your Discord bot's application client ID
GUILD_ID= # Discord server (guild) ID where the bot will be used

# 🚪 App Port
PORT= # Port the app should run on (e.g. 6173 or 1657)
